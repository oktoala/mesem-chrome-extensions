{
  "name": "mesem",
  "description": "Gboard-Smiles Clone. This extension will help you to express what you feel right now",
  "version": "1.3",
  "default_locale": "en",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": "Alt+Shift+M",
      "description": "Toggle Mesem"
    }
  },
  "icons": {
    "16": "/icons/get_started16.png",
    "32": "/icons/get_started32.png",
    "48": "/icons/get_started48.png",
    "128": "/icons/get_started128.png"
  },
  "action": {
    "default_title": "Mesem",
    "default_icon": {
      "16": "/icons/get_started16.png",
      "32": "/icons/get_started32.png",
      "48": "/icons/get_started48.png",
      "128": "/icons/get_started128.png"
    },
    "default_popup": "index.html"
  }
}